<div class="">
<div class="container body">



 <div class="container">

<h3>List of all SIP users</h3>
<table class="table table-condensed">
	<thead>
	<tr>
		<th>#</th>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Username</th>
		<th>Status</th>
		<th></th>
	</tr>
	</thead>
	<tbody>
		<% counter = 1 %>
<% @users.each do |user| %>
  
	  <tr>
	  		<td><%= counter %></td>
	  	<td><%= user.name %></td>
	  	<td><%= user.surname %></td>
	  	<td><%= user.username %></td>
	  	<td><span class="label label-success">Online</span></td>
<td>
	<form action="/clicktocall" method="post">
<input name="clickToCallID" type="hidden" value="<%= user.id%>"  />
<button class="btn btn-primary" type="submit">Click 2 Call</button>
		</form>
</td>

  </tr>
  <% counter = counter + 1 %>
  <% end %>
</tbody>


</table>
</div>
</div>
</div>